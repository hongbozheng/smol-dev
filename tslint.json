{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-debugger": false,
    "no-unused-variable": [
      true
    ],
    "no-var-keyword": true,
    "no-empty": false,
    "quotemark": [
      true,
      "single"
    ]
  },
  "rulesDirectory": []
}